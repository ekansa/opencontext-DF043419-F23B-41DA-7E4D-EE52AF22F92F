          <oc:var_label type="alphanumeric">Condition</oc:var_label>
          <oc:show_val>Face broken diagonally; somewhat encrusted with mud.  Red and white paint preserved.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>7DBB5CB7-599F-42D5-61EE-1955CF898990</arch:variableID>
