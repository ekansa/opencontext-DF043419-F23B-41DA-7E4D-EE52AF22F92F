          <oc:var_label type="alphanumeric">Size</oc:var_label>
          <oc:show_val>Max. Pres. Length (tip of nose to back of head) 0.115 m, Max. Pres. Width 0.092 m, Max. Pres. Height 0.063 m, Max. Pres. Thickness (through nose) 0.035 m</oc:show_val>
        </arch:property>
