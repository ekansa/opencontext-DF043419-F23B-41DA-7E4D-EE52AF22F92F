          <oc:propid href="http://opencontext.org/properties/01BC92D5-EC09-4B76-D1A6-883C86B93C59">01BC92D5-EC09-4B76-D1A6-883C86B93C59</oc:propid>
          <oc:var_label type="alphanumeric">Fabric Description</oc:var_label>
          <oc:show_val>Coarse clay with bits of stone fired orange-brown.</oc:show_val>
        </arch:property>
